#! ROHF frontier orbitals of CH2(s) and CH2(t).

molecule ch2s {
0 1
 C
 H 1 1.109
 H 1 1.109 2 109.5
}


molecule ch2t {
0 3
 C
 H 1 1.109
 H 1 1.109 2 109.5
}

set basis cc-pvdz
set df_scf_guess  false
set scf_type pk
set cubeprop_tasks ['frontier_orbitals']
set cubic_grid_overage [1.0,1.0,1.0]
set e_convergence 9
set d_convergence 8

set reference rhf
scf_e, scf_wfn = energy('scf', return_wfn=True, molecule=ch2s)
cubeprop(scf_wfn)


set reference rohf
scf_e, scf_wfn = energy('scf', return_wfn=True, molecule=ch2t)
cubeprop(scf_wfn)

